{
  "name": "sote-framework",
  "displayName": "SOTE Framework",
  "description": "VSCode syntax highlighting and autocomplete for the SOTE framework",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.49.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/index.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "activationEvents": [
    "onLanguage:tpe"
  ],
  "publisher": "paulpopat",
  "contributes": {
    "languages": [
      {
        "id": "tpe",
        "aliases": [
          "SOTE Framework",
          "SOTE",
          "tpe"
        ],
        "extensions": [
          ".tpe"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tpe",
        "path": "./syntaxes/tpe.html.json",
        "scopeName": "source.tpe",
        "embeddedLanguages": {
          "source.css.scss": "scss",
          "source.js": "javascript"
        }
      }
    ]
  },
  "repository": {
    "url": "https://github.com/PaulPopat/sote-vscode.git",
    "type": "GIT"
  },
  "dependencies": {
    "@paulpopat/safe-type": "^2.1.0",
    "@paulpopat/sote": "^0.6.12",
    "jayson": "^3.3.4",
    "typescript-language-server": "^0.4.0",
    "vscode-css-languageservice": "^4.3.4",
    "vscode-html-languageservice": "^3.1.3",
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver": "^6.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.11.1",
    "@types/vscode": "^1.49.0",
    "typescript": "^4.0.3"
  }
}
